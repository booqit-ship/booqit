
{
  "name": "BooqIt | Booking Made Simple",
  "short_name": "booqit",
  "id": "/",
  "description": "BooqIt - The ultimate booking platform for services in India. Book appointments with your favorite services in just a few taps.",
  "start_url": "/",
  "scope": "/",
  "dir": "ltr",
  "lang": "en",
  "orientation": "portrait",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "fullscreen", "standalone", "minimal-ui", "browser"],
  "background_color": "#ffffff",
  "theme_color": "#7E57C2",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "prefer_related_applications": false,

  "launch_handler": {
    "client_mode": "navigate-existing"
  },

  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.booqit.app",
      "id": "com.booqit.app"
    }
  ],

  "scope_extensions": [
    {
      "origin": "https://booqit.com"
    },
    {
      "origin": "https://www.booqit.com"
    }
  ],

  "shortcuts": [
    {
      "name": "Book a Service",
      "short_name": "Book",
      "description": "Quickly book your appointment",
      "url": "/",
      "icons": [{ "src": "/take_this_icons_and_files/android/android-launchericon-192-192.png", "sizes": "192x192" }]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming appointments",
      "url": "/customer/calendar",
      "icons": [{ "src": "/take_this_icons_and_files/android/android-launchericon-192-192.png", "sizes": "192x192" }]
    },
    {
      "name": "Find Services",
      "short_name": "Search",
      "description": "Search for services near you",
      "url": "/customer/search",
      "icons": [{ "src": "/take_this_icons_and_files/android/android-launchericon-192-192.png", "sizes": "192x192" }]
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+booqit",
      "url": "/?q=%s"
    }
  ],

  "categories": ["beauty", "booking", "lifestyle", "productivity", "business"],

  "screenshots": [
    {
      "src": "/screenshots/home-screen.png",
